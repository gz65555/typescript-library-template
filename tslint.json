{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "indent": [
      true,
      "spaces",
      2
    ],
    "ordered-imports": [
      false
    ],
    "object-literal-sort-keys": [
      false
    ],
    "arrow-parens": false,
    "no-console": true,
    "no-empty": false,
    "space-before-function-paren": false
  },
  "rulesDirectory": []
}
